(page "index.html")

(defelem toolbar-item
	[_ children]
	(div :class "toolbar-item"
			 children))

(defelem spacer [_ _] (div :class "spacer"))

(html
 (head
	(link :href "css/main.css" :rel "stylesheet"))
 (body
	(div :class "panel fill"
			 (div :class "toolbar"
